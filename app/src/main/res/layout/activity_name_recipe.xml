<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.bsu.cs222.cnj.homebrewyo2.NameRecipeActivity"
    android:background="#d3af60">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/namebutton"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:src="@drawable/dogma"
        android:background="#00ffffff"
        android:onClick="goDogmaRecipes"
        android:layout_below="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_marginTop="153dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:src="@drawable/physics"
        android:onClick="goPhysicsRecipes"
        android:background="#00ffffff"
        android:layout_below="@+id/imageButton15"
        android:layout_alignStart="@+id/imageButton15"
        android:layout_marginTop="31dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:src="@drawable/punk"
        android:background="#00ffffff"
        android:onClick="goPunkRecipes"
        android:layout_below="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton12" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton14"
        android:src="@drawable/hoprocker"
        android:background="#00ffffff"
        android:onClick="goHoprockerRecipes"
        android:layout_below="@+id/imageButton11"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton15"
        android:src="@drawable/peroxidepunk"
        android:background="#00ffffff"
        android:onClick="goPeroxidePunkIngredients"
        android:layout_below="@+id/imageButton14"
        android:layout_alignStart="@+id/imageButton14"
        android:layout_marginTop="32dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton16"
        android:layout_below="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_marginTop="34dp"
        android:src="@drawable/brixtonporter"
        android:background="#00ffffff"
        android:onClick="goBrixtonPorter"/>

</RelativeLayout>
